#include <iostream>
using namespace std;

int main() {
    int i,j,n=9;
    
    int u=n/2+1;
    int l=n/2;
    for(i=1; i<=u;i++){
        for(j=1;j<=i; j++){
            cout<<"*";
        }
                    cout<<endl;
   
    }
    
     for(i=1; i<=l;i++){
        for(j=l;j>=i; j--){
            cout<<"*";
    
        }
                cout<<endl;
   
    }
    return 0;
}



